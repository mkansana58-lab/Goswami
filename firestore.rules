rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and allow public access.
    // You should restrict these rules before launching your app to production.
    // For now, this allows the admin panel to write data.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
